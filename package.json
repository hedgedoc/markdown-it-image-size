    "@typescript-eslint/parser": "5.33.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.8.2",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "markdown-it": "13.0.1",
