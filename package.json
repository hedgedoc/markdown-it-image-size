{
  "name": "@hedgedoc/markdown-it-image-size",
  "version": "1.0.1",
  "description": "A markdown-it plugin add image sizes ported to typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSES/*",
    "package.json",
    "README.md",
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublish": "tsc",
    "lint": "eslint . --ext .ts"
  },
  "keywords": ["markdown", "markdown-it", "image size"],
  "author": "The HedgeDoc developers",
  "license": "AGPL-3.0",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/markdown-it": "^12.2.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "jest": "^27.1.0",
    "markdown-it": "^12.2.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hedgedoc/markdown-it-image-size.git"
  },
  "bugs": {
    "url": "https://github.com/hedgedoc/markdown-it-image-size/issues"
  },
  "homepage": "https://github.com/hedgedoc/markdown-it-image-size#readme"
}
