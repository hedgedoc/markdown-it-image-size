{
  "name": "@hedgedoc/markdown-it-image-size",
  "version": "1.0.3",
  "description": "A markdown-it plugin add image sizes ported to typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSES/*",
    "package.json",
    "README.md",
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublish": "tsc",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "image size"
  ],
  "author": "The HedgeDoc developers",
  "license": "AGPL-3.0",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "28.1.4",
    "@types/markdown-it": "12.2.3",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "eslint": "8.18.0",
    "jest": "28.1.2",
    "markdown-it": "13.0.1",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hedgedoc/markdown-it-image-size.git"
  },
  "bugs": {
    "url": "https://github.com/hedgedoc/markdown-it-image-size/issues"
  },
  "homepage": "https://github.com/hedgedoc/markdown-it-image-size#readme",
  "packageManager": "yarn@3.2.1"
}
